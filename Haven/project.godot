; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Haven"
run/main_scene="res://Scenes/Login Page.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.svg"

[autoload]

Global="*res://Global/Global.gd"
Firebase="*res://addons/godot-firebase/firebase/firebase.tscn"

[display]

window/size/viewport_width=1440
window/size/viewport_height=2560
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="Haven"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-firebase/plugin.cfg")

[rendering]

renderer/rendering_method="mobile"
